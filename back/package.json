    "dependencies": {
        "cores": "^0.8.5",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "http-proxy-middleware": "^2.0.6",
        "sequelize": "^6.37.1",
        "sqlite": "^4.1.2",
        "sqlite3": "^5.1.7",
        "uuid": "^9.0.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.18",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^9.0.7",
        "axios": "^1.2.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^4.9.4"
